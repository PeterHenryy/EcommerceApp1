@model Company

<table class="table table-striped table-dark">
  <tr>
    <th>Products In Stock</th>
    <th>Logo</th>
    <th>Total Sales</th>
    <th>Company Revenue</th>
    <th>Action</th>
    <th>Action</th>
  </tr>

    <tr>
        <td>@Model.ProductsInStock</td>
        <td> <img style="width: 200px;" src="@Model.Logo"> </td>
        <td>@Model.TotalSales</td>
        <td>$@Model.Revenue</td>
        <td> <a class="btn btn-secondary" href="@Url.Action("Update", "Company", new {companyID = @Model.ID})">Update</a></td>
        <td> <a class="btn btn-danger" href="@Url.Action("Delete", "Company", new {companyID = @Model.ID})">Delete</a></td>
    </tr>
    

</table>
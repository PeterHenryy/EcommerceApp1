@model IEnumerable<Company>

<table class="table table-striped table-dark">
    <tr>
        <th>Name</th>
        <th>Products In Stock</th>
        <th>Logo</th>
        <th>Total Sales</th>
    </tr>

    @foreach(var company in @Model)
    {
        <tr>
            <td>@company.Name</td>
            <td>@company.ProductsInStock</td>
            <td> <img style="width: 200px;" src="@company.Logo"> </td>
            <td>@company.TotalSales</td>
        </tr>
    }

</table>